/dts-v1/;

/ {
	compatible = "brcm,bcm2835";

	fragment@0 {
		target = <0xffffffff>;

		__overlay__ {
			status = "okay";
		};
	};

	fragment@1 {
		target = <0xffffffff>;

		__overlay__ {
			#address-cells = <0x01>;
			#size-cells = <0x00>;
			status = "okay";

			tas5711@1a {
				#sound-dai-cells = <0x00>;
				compatible = "ti,tas5711";
				reg = <0x1a>;
				status = "okay";
				pdn-gpios = <0xffffffff 0x17 0x01>;
				reset-gpios = <0xffffffff 0x18 0x01>;
			};
		};
	};

	fragment@2 {
		target = <0xffffffff>;

		__overlay__ {
			compatible = "pifi,pifi-mini-210";
			i2s-controller = <0xffffffff>;
			status = "okay";
		};
	};

	__fixups__ {
		i2s = "/fragment@0:target:0\0/fragment@2/__overlay__:i2s-controller:0";
		i2c1 = "/fragment@1:target:0";
		gpio = "/fragment@1/__overlay__/tas5711@1a:pdn-gpios:0\0/fragment@1/__overlay__/tas5711@1a:reset-gpios:0";
		sound = "/fragment@2:target:0";
	};
};

/dts-v1/;

/ {
	compatible = "brcm,bcm2835";

	fragment@0 {
		target = <0xffffffff>;

		__overlay__ {
			status = "okay";
		};
	};

	fragment@1 {
		target = <0xffffffff>;

		__overlay__ {
			#address-cells = <0x01>;
			#size-cells = <0x00>;
			status = "okay";

			wm8804@3b {
				#sound-dai-cells = <0x00>;
				compatible = "wlf,wm8804";
				reg = <0x3b>;
				PVDD-supply = <0xffffffff>;
				DVDD-supply = <0xffffffff>;
				status = "okay";
			};
		};
	};

	fragment@2 {
		target = <0xffffffff>;

		__overlay__ {
			#address-cells = <0x01>;
			#size-cells = <0x00>;
			status = "okay";

			pcm5122@4d {
				#sound-dai-cells = <0x00>;
				compatible = "ti,pcm5122";
				reg = <0x4d>;
				AVDD-supply = <0xffffffff>;
				DVDD-supply = <0xffffffff>;
				CPVDD-supply = <0xffffffff>;
				status = "okay";
			};
		};
	};

	fragment@3 {
		target = <0xffffffff>;

		__overlay__ {
			compatible = "justboom,justboom-both";
			i2s-controller = <0xffffffff>;
			status = "okay";
			phandle = <0x01>;
		};
	};

	__overrides__ {
		24db_digital_gain = <0x01 0x6a757374 0x626f6f6d 0x2c323464 0x625f6469 0x67697461 0x6c5f6761 0x696e3f00>;
	};

	__symbols__ {
		frag3 = "/fragment@3/__overlay__";
	};

	__fixups__ {
		i2s = "/fragment@0:target:0\0/fragment@3/__overlay__:i2s-controller:0";
		i2c1 = "/fragment@1:target:0\0/fragment@2:target:0";
		vdd_3v3_reg = "/fragment@1/__overlay__/wm8804@3b:PVDD-supply:0\0/fragment@1/__overlay__/wm8804@3b:DVDD-supply:0\0/fragment@2/__overlay__/pcm5122@4d:AVDD-supply:0\0/fragment@2/__overlay__/pcm5122@4d:DVDD-supply:0\0/fragment@2/__overlay__/pcm5122@4d:CPVDD-supply:0";
		sound = "/fragment@3:target:0";
	};

	__local_fixups__ {

		__overrides__ {
			24db_digital_gain = <0x00>;
		};
	};
};

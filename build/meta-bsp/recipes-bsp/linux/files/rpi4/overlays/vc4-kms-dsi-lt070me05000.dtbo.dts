	};

	__symbols__ {
		dsi_out_port = "/fragment@0/__overlay__/port/endpoint";
		lt070me05000 = "/fragment@0/__overlay__/lt070me05000@0";
		panel_dsi_port = "/fragment@0/__overlay__/lt070me05000@0/port/endpoint";
		lt070me05000_pins = "/fragment@1/__overlay__/lt070me05000_pins";
	};

	__fixups__ {
		dsi1 = "/fragment@0:target:0";
		gpio = "/fragment@0/__overlay__/lt070me05000@0:reset-gpios:0\0/fragment@0/__overlay__/lt070me05000@0:enable-gpios:0\0/fragment@0/__overlay__/lt070me05000@0:dcdc-en-gpios:0\0/fragment@1:target:0";
	};

	__local_fixups__ {

		fragment@0 {

			__overlay__ {

				port {

					endpoint {
						remote-endpoint = <0x00>;
					};
				};

				lt070me05000@0 {

					port {

						endpoint {
							remote-endpoint = <0x00>;
						};
					};
				};

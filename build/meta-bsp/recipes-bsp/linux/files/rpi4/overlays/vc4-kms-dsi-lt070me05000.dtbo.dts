				reg = <0x00>;
				reset-gpios = <0xffffffff 0x11 0x01>;
				enable-gpios = <0xffffffff 0x04 0x00>;
				dcdc-en-gpios = <0xffffffff 0x05 0x00>;
				phandle = <0x04>;

				port {

					endpoint {
						remote-endpoint = <0x02>;
						phandle = <0x01>;
					};
				};
			};
		};
	};

	fragment@1 {
		target = <0xffffffff>;

		__overlay__ {

